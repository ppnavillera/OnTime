<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView
		android:id="@+id/rld06yh25erk"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:clipToPadding="false"
		android:paddingTop="195dp">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">
			<TextView
				android:id="@+id/ri6ozrqmt6no"
				android:textColor="#000000"
				android:textSize="32sp"
				android:layout_marginBottom="19dp"
				android:layout_marginStart="41dp"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:text="Sign Up"
			/>
			<TextView
				android:id="@+id/rfg9u7t1npz7"
				android:textColor="#000000"
				android:textSize="12sp"
				android:layout_marginBottom="32dp"
				android:layout_marginStart="40dp"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:text="Welcome to the Tardiness Prevention Challenge!"
			/>
			<EditText
				android:id="@+id/editTextName"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="39dp"
				android:layout_marginBottom="19dp"
				android:background="@drawable/cr15be8e8e899"
				android:hint="Name"
				android:inputType="text"
				android:textColor="#000000"
				android:textSize="16sp"
				android:paddingHorizontal="16dp"
				android:paddingVertical="14dp"/>
			<EditText
				android:id="@+id/editTextPhoneNumber"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="39dp"
				android:layout_marginBottom="19dp"
				android:background="@drawable/cr15be8e8e899"
				android:hint="Phone Number"
				android:inputType="phone"
				android:textColor="#000000"
				android:textSize="16sp"
				android:paddingHorizontal="16dp"
				android:paddingVertical="14dp"/>
			<EditText
				android:id="@+id/editTextPassword"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/cr15be8e8e899"
				android:hint="Password"
				android:inputType="textPassword"
				android:textColor="#000000"
				android:paddingHorizontal="16dp"
				android:paddingVertical="14dp"
				android:layout_marginHorizontal="39dp"
				android:layout_marginBottom="19dp"
				android:textSize="16sp" />
			<EditText
				android:id="@+id/editTextConfirmPassword"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/cr15be8e8e899"
				android:hint="Confirm Password"
				android:inputType="textPassword"
				android:textColor="#000000"
				android:paddingHorizontal="16dp"
				android:paddingVertical="14dp"
				android:layout_marginHorizontal="39dp"
				android:layout_marginBottom="19dp"
				android:textSize="16sp" />

			<Button
				android:id="@+id/signUpButton"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="39dp"
				android:background="@drawable/cr10bff6e70"
				android:paddingVertical="13dp"
				android:text="Sign Up"
				android:textColor="#FFFAFA"
				android:textSize="16sp" />
		</LinearLayout>
	</ScrollView>
</LinearLayout>
